<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime targetFramework="4.5.1" />
  </system.web>
  <system.webServer>
    <modules>
      <add name="ParamsProtection" type="Events.ParamsModule"/>
      <add name="AverageTime" type="Events.AverageTimeModule"/>
    
    </modules>
  </system.webServer>
</configuration>